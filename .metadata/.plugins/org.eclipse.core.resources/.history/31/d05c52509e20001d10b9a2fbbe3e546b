package IEmployee;

public class Entry {
public static void main(String[] args) {
	IEmployee employee1 = new PartTimeEmployee("duc",4000,7);
	System.out.println("Name: " + employee1.getName());
	System.out.println("Salary per day: " + employee1.calculateSalary());
	
	IEmployee employee2 = new FullTimeEmployee("dat",3000);
	System.out.println("Name: " + employee2.getName());
	System.out.println("Salary per day: " + employee2.calculateSalary());
	
}
}
