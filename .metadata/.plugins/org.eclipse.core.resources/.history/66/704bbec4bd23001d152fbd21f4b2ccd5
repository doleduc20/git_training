package model;

public class LastButtonModel {
	private int value;
	
	public LastButtonModel(int value) {
		this.value=value;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}
	public void num_1() {
		this.setValue(1);
	}
	public void num_2() {
		this.setValue(1);
	}
	public void num_3() {
		this.setValue(3);
	}
	public void num_4() {
		this.setValue(4);
	}
}
